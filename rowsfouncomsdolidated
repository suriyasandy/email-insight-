rows_found = False
with open(file, newline="", encoding="utf-8") as f:
    reader = csv.DictReader(f)
    for row in reader:
        out_row = [trade_id]
        for header in static_headers[1:]:
            out_row.append(row.get(header, ""))
        ws.append(out_row)
        rows_found = True

# If no data rows found, still write the trade_id (with empty columns)
if not rows_found:
    out_row = [trade_id] + [""] * (len(static_headers) - 1)
    ws.append(out_row)
